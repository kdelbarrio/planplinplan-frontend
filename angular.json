{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "plan-plin-plan": {
      "projectType": "application",
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "defaultConfiguration": "production",
          "options": {
            "browser": "src/main.ts",
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "polyfills": ["zone.js"],
            "outputPath": "dist/plan-plin-plan/browser",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              { "glob": "**/*", "input": "public" },
              "src/manifest.webmanifest"
            ],
            "styles": ["src/styles.scss"]
            
          },
          "configurations": {
            "development": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.development.ts"
                }
              ],
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            },
            "production": {
              "optimization": true,
              "sourceMap": false,
              "extractLicenses": true,
              "outputHashing": "all",
              "serviceWorker": "ngsw-config.json"
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "defaultConfiguration": "development",
          "options": {
            "buildTarget": "plan-plin-plan:build:development"
          },
          "configurations": {
            "development": {
              "buildTarget": "plan-plin-plan:build:development"
            },
            "production": {
              "buildTarget": "plan-plin-plan:build:production"
            }
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "16d6144a-ab6f-4242-827a-8d6fdd9933a9"
  }
}
